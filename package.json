{
  "name": "ananda-library-chatbot",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Michael Olivier",
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "build-with-tests": "NODE_ENV=development jest && node scripts/build.js",
    "build-with-api-tests": "NODE_ENV=development jest --config=jest.config.cjs && node scripts/build.js",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx)\" --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "ingest": "NODE_OPTIONS='--max-old-space-size=6000' tsx -r dotenv/config scripts/ingest-text-data.ts",
    "ingest-trace": "NODE_OPTIONS='--max-old-space-size=6000 --trace-deprecation' tsx -r dotenv/config scripts/ingest-text-data.ts",
    "emulator": "firebase emulators:start --import ./emulator-data --export-on-exit ./emulator-data --inspect-functions",
    "prompt": "tsx -r dotenv/config scripts/manage-prompts.ts"
  },
  "dependencies": {
