{
  "name": "@mega-rag-chatbot/web",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Michael Olivier",
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:site": "SITE_ID=$npm_config_site next dev",
    "install-deps": "cd .. && npm install",
    "build": "next build",
    "build-with-tests": "NODE_ENV=development jest && next build",
    "build-with-api-tests": "NODE_ENV=development jest && jest --selectProjects=server && node scripts/build.js ananda",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx)\" --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:changed": "jest --bail --findRelatedTests",
    "test:server": "jest --selectProjects=server",
    "test:all": "npm test && echo '✅ Client tests passed! Running server tests...' || (echo '❌ Client tests failed!' && exit 1) && npm run test:server",
    "test:server:coverage": "jest --selectProjects=server --coverage",
    "test:queries:ananda-public": "RUN_SEMANTIC_TESTS=true RUN_LOCATION_TESTS=true dotenv -e ../.env.ananda-public -- jest -c jest.config.cjs __tests__/site_specific/ananda-public/*.test.ts",
    "test:location:ananda-public": "RUN_LOCATION_TESTS=true dotenv -e ../.env.ananda-public -- jest -c jest.config.cjs __tests__/site_specific/ananda-public/locationSemantic.test.ts",
    "find-untested": "node ../scripts/find-untested-files.js",
    "build:location-intent": "tsx scripts/generate-location-intent-embeddings.ts",
    "emulator": "firebase emulators:start --import ../emulator-data --export-on-exit ../emulator-data --inspect-functions",
    "prepare": "cd .. && command -v husky >/dev/null 2>&1 && husky || true",
    "setup-hooks": "cd .. && husky",
    "prompt": "tsx -r dotenv/config scripts/manage-prompts.ts"
  },
  "dependencies": {
    "@langchain/core": "^0.3.78",
    "@pinecone-database/pinecone": "^5.1.2",
    "@tailwindcss/typography": "0.5.14",
    "@tanstack/react-query": "^5.90.2",
    "@types/email-templates": "^10.0.4",
