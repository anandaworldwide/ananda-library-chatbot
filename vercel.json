{
  "crons": [
    {
      "path": "/api/relatedQuestions?updateBatch=100",
      "schedule": "0 */2 * * *"
    }
  ],
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next",
      "config": {
        "buildCommand": "cd web && npm run build-with-api-tests"
      }
    }
  ],
  "outputDirectory": "web/.next"
}
