{
  "rootDirectory": "web",
  "crons": [
    {
      "path": "/api/relatedQuestions?updateBatch=100",
      "schedule": "0 */2 * * *"
    }
  ],
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install --no-package-lock --no-audit --ignore-scripts",
  "outputDirectory": ".next"
}
